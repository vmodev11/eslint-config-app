{
  "name": "react-test-eslint",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com-huyhavmodev:vmodev11/react-test-eslint.git",
  "author": "huyha <huy.ha@vmodev.com>",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts?(x)\" \"test/**/*.ts?(x)\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start:debug": "ts-node-dev --files -r tsconfig-paths/register ./src/main.ts",
    "lint": "eslint \"{src,test}/**/*.ts?(x)\""
  },
  "dependencies": {
    "express": "^4.17.1",
    "fbgraph": "^1.4.4",
    "helmet": "^4.6.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
